{
  "name": "@chatflow/backend",
  "version": "1.0.0",
  "description": "ChatFlow backend API server with Firestore and GCP Pub/Sub",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "emulators:start": "gcloud emulators firestore start --host-port=localhost:8080 & gcloud emulators pubsub start --host-port=localhost:8085",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "dependencies": {
    "@chatflow/shared": "file:../shared",
    "firebase-admin": "^12.0.0",
    "@google-cloud/pubsub": "^4.0.0",
    "@google-cloud/discoveryengine": "^1.7.0",
    "@google-cloud/documentai": "^8.7.0",
    "@google-cloud/aiplatform": "^3.21.0",
    "express": "^4.18.0",
    "express-validator": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "ws": "^8.14.0",
    "swagger-jsdoc": "^6.2.0",
    "swagger-ui-express": "^5.0.0",
    "yamljs": "^0.3.0",
    "dotenv": "^16.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.0",
    "@types/ws": "^8.5.0",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.0",
    "@types/yamljs": "^0.2.0",
    "@types/jest": "^29.5.0",
    "@types/supertest": "^2.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "supertest": "^6.3.0",
    "tsx": "^3.12.0",
    "typescript": "^5.0.0",
    "eslint": "^8.45.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  }
} 